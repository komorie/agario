<?xml version="1.0" encoding="utf-8" ?>
<PDL>
  <packet name ="S_BroadcastEnterGame">
    <!--야 얘 로그인했다ㅋㅋ-->
    <int name="playerId"/>
    <float name="posX"/>
    <float name="posY"/>
    <float name="posZ"/>
  </packet>

  <packet name="C_LeaveGame">
    <!--저 나갑니다-->
  </packet>

  <packet name="S_BroadcastLeaveGame">
    <!--얘 나간다~-->
    <int name="playerId"/>
  </packet>

  <packet name="S_BroadcastServerTime">
    <float name="serverTime"/>
  </packet>

  <packet name="S_RoomList">
    <!--현재 방에 있는 모든 정보 알려준다-->
    <list name="player">
      <bool name="isSelf"/> <!--받는 클라랑 같은 플레이어인지 여부-->
      <int name="playerId"/>
      <float name="dirX"/>
      <float name="dirY"/>   <!--현재 이동 방향, 크기-->
      <float name="posX"/>
      <float name="posY"/>
      <float name="posZ"/>
      <float name="radius"/> <!--캐릭터 크기-->
    </list>
    <list name="food">
      <!--음식들 위치-->
      <int name="foodType"/>
      <float name="posX"/>
      <float name="posY"/>
    </list>
  </packet>

  <packet name="C_Move"> <!--저 어디서부터 이렇게 이동할게요-->
    <float name="dirX"/>
    <float name="dirY"/>
    <float name="posX"/>
    <float name="posY"/>
    <float name="posZ"/>
    <float name="time"/>
  </packet>

  <packet name="S_BroadcastMove"> <!--얘 어디서부터 이렇게 이동한다-->
    <int name="playerId"/>
    <float name="dirX"/>
    <float name="dirY"/>
    <float name="posX"/>
    <float name="posY"/>
    <float name="posZ"/>
    <float name="time"/>
  </packet>

  <packet name="C_EatFood">
    <!--제가 이 음식 먹었으묘-->
    <int name ="foodId"/>
  </packet>

  <packet name="S_BroadcastEatFood">
    <!--얘가 음식 먹었고, 해당 음식의 새 위치는 여기임-->
    <int name="playerId"/>
    <int name="foodId"/>
    <float name="posX"/>
    <float name="posY"/>
  </packet>
  
  <packet name="C_EatPlayer">
    <!--제가 이 녀석 먹었음-->
    <int name ="predatorId"/>
    <int name ="preyId"/>
  </packet>


  <packet name="S_BroadcastEatPlayer">
    <!--얘가 이 녀석 먹었다-->
    <int name ="predatorId"/>
    <int name ="preyId"/>
  </packet>
</PDL>
